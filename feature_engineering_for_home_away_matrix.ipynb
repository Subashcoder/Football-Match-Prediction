    "import numpy as np\n",
    "\n",
    "def preprocess_data_for_prediction(player_stats):\n",
    "    # Ensure that the array has at least 88 elements\n",
    "    required_length = 88\n",
    "    if player_stats.size < required_length:\n",
    "        raise ValueError(f\"Not enough player statistics. Expected at least {required_length} elements.\")\n",
    "\n",
    "    # Flatten the array to handle nested structure (e.g., Series within DataFrame)\n",
    "    flattened_values = player_stats.ravel()\n",
    "    \n",
    "    # Take the first 88 elements (or pad with zeros if necessary)\n",
    "    processed_data = flattened_values[:required_length]\n",
    "    \n",
    "    # Reshape the processed data to have shape (1, 88)\n",
    "    processed_data = processed_data.reshape(1, -1)\n",
    "    \n",
    "    return processed_data\n",
    "\n",
    "# Example usage:\n",
    "# Assuming `player_stats` is your numpy array containing player statistics\n",
    "# player_stats = ...  # Your numpy array containing player statistics\n",
    "\n",
    "processed_data = preprocess_data_for_prediction(player_stats)\n",
    "processed_data\n",
    "\n"
   ]
