    "import requests\n",
    "\n",
    "def get_player_info(player_id):\n",
    "    # Construct the API endpoint URL for player data\n",
    "    player_url = f'https://fantasy.premierleague.com/api/element-summary/{player_id}/'\n",
    "    \n",
    "    # Make a GET request to retrieve player data\n",
    "    response = requests.get(player_url)\n",
    "    player_data = response.json()\n",
    "    \n",
    "    return player_data\n",
    "\n",
    "def get_next_fixture(player_data):\n",
    "    # Find the next fixture for the player's team\n",
    "    fixtures = player_data['fixtures']\n",
    "    \n",
    "    # Iterate through fixtures to find the upcoming fixture\n",
    "    for fixture in fixtures:\n",
    "        if 'event' in fixture and 'next_event_fixture' in player_data and fixture['event'] == player_data['next_event_fixture']:\n",
    "            return fixture\n",
    "    \n",
    "    return None\n",
    "\n",
    "def assess_player_availability(player_id):\n",
    "    # Retrieve player information\n",
    "    player_data = get_player_info(player_id)\n",
    "    \n",
    "    if 'error' in player_data:\n",
    "        print(f\"Error: {player_data['error']}\")\n",
    "        return\n",
    "    \n",
    "    # Get the next fixture for the player's team\n",
    "    next_fixture = get_next_fixture(player_data)\n",
    "    \n",
    "    if next_fixture is None:\n",
    "        print(\"Next fixture not found.\")\n",
    "        return\n",
    "    \n",
    "    # Assess player availability based on fixture details\n",
    "    opponent = next_fixture['opponent_name']\n",
    "    kickoff_time = next_fixture['kickoff_time']\n",
    "    \n",
    "    print(f\"Next Fixture: {opponent} (Kickoff: {kickoff_time})\")\n",
    "    \n",
    "    # Example logic to assess player availability (customize as needed)\n",
    "    # Here, you could use various factors to determine player availability\n",
